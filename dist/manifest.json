{
  "name": "Cookie Jar",
  "version": "0.1.6",
  "description": "Erase all browsing data except data useful for your bookmarks.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Options"
  },
  "icons": {
    "128": "icon256.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "bookmarks",
    "browsingData"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
